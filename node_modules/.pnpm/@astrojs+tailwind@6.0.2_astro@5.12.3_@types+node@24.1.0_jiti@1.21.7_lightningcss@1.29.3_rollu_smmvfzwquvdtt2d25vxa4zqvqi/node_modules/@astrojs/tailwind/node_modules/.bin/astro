#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yhoaua/yhoaua.github.io/node_modules/.pnpm/astro@5.12.3_@types+node@24.1.0_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_sty_3ps6uy43wqnlw2yruriywlytyq/node_modules/astro/node_modules:/Users/yhoaua/yhoaua.github.io/node_modules/.pnpm/astro@5.12.3_@types+node@24.1.0_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_sty_3ps6uy43wqnlw2yruriywlytyq/node_modules:/Users/yhoaua/yhoaua.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yhoaua/yhoaua.github.io/node_modules/.pnpm/astro@5.12.3_@types+node@24.1.0_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_sty_3ps6uy43wqnlw2yruriywlytyq/node_modules/astro/node_modules:/Users/yhoaua/yhoaua.github.io/node_modules/.pnpm/astro@5.12.3_@types+node@24.1.0_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_sty_3ps6uy43wqnlw2yruriywlytyq/node_modules:/Users/yhoaua/yhoaua.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.12.3_@types+node@24.1.0_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_sty_3ps6uy43wqnlw2yruriywlytyq/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.12.3_@types+node@24.1.0_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.80.4_sty_3ps6uy43wqnlw2yruriywlytyq/node_modules/astro/astro.js" "$@"
fi
